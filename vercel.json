{
  "version": 2,
  "name": "FotoStudio SMTP",
  "builds": [
     { "src": "contactForm.js", "use": "@vercel/node" }
  ],
  "redirects": [
      {
         "source": "/kontakt/send",
         "dest": "contactForm.js",
         "has": [
            {
              "type": "header",
              "key": ":method",
              "value": "POST"
            }
          ]
      }
   ]
}